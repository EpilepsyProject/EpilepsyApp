<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.promobile.epilepticdetector"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.promobile.epilepticdetector.SplashActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.promobile.epilepticdetector.MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.promobile.epilepticdetector.ListView_Add"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.promobile.epilepticdetector.ConfiguracaoActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.promobile.epilepticdetector.HistoricoActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.promobile.epilepticdetector.InicialActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.promobile.epilepticdetector.ListView_Config"
            android:label="@string/title_activity_list_view" >
        </activity>
        <activity
            android:name="com.promobile.epilepticdetector.Config_AlertasActivity"
            android:label="@string/title_activity_config__alertas" >
        </activity>
        <activity
            android:name="com.promobile.epilepticdetector.AlertasActivity"
            android:label="@string/title_activity_alertas" >
        </activity>
        <activity
            android:name="com.promobile.epilepticdetector.ContatosActivity"
            android:label="@string/title_activity_contatos" >
        </activity>
        <activity
            android:name="com.promobile.epilepticdetector.TelaDesmaio"
            android:label="@string/title_activity_tela_desmaio" >
        </activity>
        <activity
            android:name="com.promobile.epilepticdetector.AcelerometroActivity"
            android:label="@string/title_activity_acelerometro" >
        </activity>
    </application>

</manifest>
