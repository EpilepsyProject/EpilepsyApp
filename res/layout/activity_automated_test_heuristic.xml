<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/LinearLayout1"
    android:layout_width="fill_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".AutomatedTestHeuristicActivity" >

    <!-- TODO: NAO REMOVER O BOTAO, POIS PARA DEBUGAR A HEURISTICA DE DESMAIO EH MAIS FACIL DEVIDO OS LOGS!!!! -->

    <Button
        android:id="@+id/btnStartTest"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"   
        android:layout_marginBottom="10dp"
        android:text="@string/btnStartTest"
        android:textColor="#ffffffff" />

    <TextView
        android:id="@+id/txtChaveTeste"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/txt_chave_teste" />

    <TextView
        android:id="@+id/txtValorTimer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/txt_tempo" />

    <TextView
        android:id="@+id/txtValorX"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/txt_valor_x_ace" />

    <TextView
        android:id="@+id/txtValorY"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/txt_valor_y_ace" />

    <TextView
        android:id="@+id/txtValorZ"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/txt_valor_z_ace" />

    <TextView
        android:id="@+id/txtValorXG"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/txt_valor_x_giro" />

    <TextView
        android:id="@+id/txtValorYG"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/txt_valor_y_giro" />

    <TextView
        android:id="@+id/txtValorZG"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/txt_valor_z_giro" />

    <TextView
        android:id="@+id/txtValorVetor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/txt_valor_vetor" />

    <TextView
        android:id="@+id/text_view_status"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="#C3C3C3" />
    
</LinearLayout>